{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "public/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/p/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}